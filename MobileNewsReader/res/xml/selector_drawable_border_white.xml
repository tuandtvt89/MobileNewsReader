        <shape>
            <solid android:color="#fff14d67" />
            <stroke android:width="1.0px" android:color="#ffebebeb" />
            <padding android:left="1.0dip" android:top="2.0dip" android:right="1.0dip" android:bottom="2.0dip" />
            <corners android:topLeftRadius="17.0sp" android:topRightRadius="17.0sp" android:bottomLeftRadius="17.0sp" android:bottomRightRadius="17.0sp" />
        </shape>
    </item>
    <item android:state_focused="true">
